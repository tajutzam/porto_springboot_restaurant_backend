{
  "openapi": "3.0.1",
  "info": {
    "title": "Sample API",
    "description": "API description in Markdown.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "localhost:8081/api/user/menu"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "paths": {
    "/": {
      "get": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "summary": "Returns a list of menu available.",
        "description": "show menu when status is available or ready.",
        "responses": {
          "200": {
            "description": "success create new menu",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "properties": {
                          "id": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          },
                          "status": {
                            "type": "string"
                          },
                          "image": {
                            "type": "string"
                          },
                          "category": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "string"
                              },
                              "name": {
                                "type": "string"
                              },
                              "images": {
                                "type": "string"
                              }
                            }
                          },
                          "restaurant": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "string"
                              },
                              "username": {
                                "type": "string"
                              },
                              "firstName": {
                                "type": "string"
                              },
                              "lastName": {
                                "type": "string"
                              },
                              "address": {
                                "type": "string"
                              },
                              "banner": {
                                "type": "string"
                              }
                            }
                          }
                        }
                      }
                    },
                    "objectPaging": {
                      "type": "object",
                      "properties": {
                        "page": {
                          "type": "integer"
                        },
                        "size": {
                          "type": "integer"
                        }
                      }
                    },
                    "message": {
                      "type": "string"
                    },
                    "status": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "unauthorized response",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "example": null
                    },
                    "message": {
                      "type": "string"
                    },
                    "status": {
                      "type": "integer",
                      "example": 401
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/order/add": {
      "post": {
        "summary": "",
        "operationId": "",
        "parameters": [
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "menuId": {
                    "type": "string"
                  },
                  "cartId": {
                    "type": "string"
                  },
                  "qty": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "success add to cart",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "properties": {
                        "id": {
                          "type": "string"
                        },
                        "qty": {
                          "type": "integer"
                        },
                        "subTotal": {
                          "type": "integer"
                        },
                        "cart": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "type": "string"
                            },
                            "status": {
                              "type": "string"
                            }
                          }
                        },
                        "menu": {
                          "properties": {
                            "id": {
                              "type": "string"
                            },
                            "images": {
                              "type": "string"
                            }
                          }
                        }
                      }
                    },
                    "message": {
                      "type": "string"
                    },
                    "status": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "bad request",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "example": null
                    },
                    "message": {
                      "type": "string"
                    },
                    "status": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "unauthorized response"
          }
        }
      }
    }
  }
}
